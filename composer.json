{
  "name": "dwenzel/command-scheduler-bundle",
  "type": "library",
  "license": [
    "GPL-2.0-or-later"
  ],
  "description": "This is an extension to the symfony CommandSchedulerBundle (`jmose/command-scheduler-bundle`) ",
  "require": {
    "php": "^7.2 | ^7.3 | ^7.4",
    "jmose/command-scheduler-bundle": "^2.1"
  },
  "require-dev": {
    "dama/doctrine-test-bundle": "^6.2",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "phpunit/phpunit": "^8.4"
  },
  "config": {
    "platform": {
      "php": "7.2"
    }
  },
  "bin": ["bin/console"],
  "autoload": {
    "psr-4": {
      "DWenzel\\CommandSchedulerBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DWenzel\\CommandSchedulerBundle\\Tests\\": "tests/"
    }
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "allow-contrib": "true",
      "require": "4.3.*"
    }
  }
}
